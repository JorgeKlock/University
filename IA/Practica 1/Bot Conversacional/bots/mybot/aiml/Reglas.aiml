<set var="cantidadtemporadas"><map name="predecessor"><get var="cantidadtemporadas"/></map></set>
</think>
<loop/>
	</li>
	</condition>
<think>
<set var="iteraciones"><srai>count <get var="temp"/></srai></set>
</think>
<condition var="iteraciones">
